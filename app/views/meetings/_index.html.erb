<div class='calendar-container col-md-8'>
  <div class='calendar'>
    <%= month_calendar events: @meetings do |date, meetings| %>
      <%= date.strftime('%d') %>
        <% meetings.each do |meeting| %>
          <div>
            <%= link_to "#{meeting.exec.name + " " + meeting.start.strftime('%I:%M %p')}", meeting_path(meeting), class: 'timeslot', data: { exec_id: meeting.exec.id } %>
          </div>
       <% end %>
    <% end %>
  </div>
</div>